package mlh;

public class Calculater {
	private double a,b;
	public double getA(){
		return a;
	}
	public double getB() {
		return b;
	}
	public void setB(double b) {
		this.b = b;
	}
	public void setA(double a) {
		this.a = a;
	}
	public Calculater(double a,double b){
		this.a=a;
		this.b=b;
	}
	public double add(){
    return a+b;
}
	public double jian(){
	    return a-b;
	}
	public double cheng(){
	    return a*b;
	}
	public double chu(){
	    return a/b;
	}
}
